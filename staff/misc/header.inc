function addUpMarkBoxes() {
    var marks = 0;
    var totalweighting =0;
    var output = "";
    for(j=0;j<document.moduleAssess.elements.length;j++){
        var formObj=document.moduleAssess.elements[j];
        if(formObj.type.toLowerCase() == "text"){
             if (isFinite(parseFloat(formObj.value))) {

                var formTotal=document.moduleAssess.elements[j+1];
                var formWeighting=document.moduleAssess.elements[j+2];
                totalweighting=totalweighting+parseFloat(formWeighting.value);
                marks=marks+(parseFloat(formObj.value)/parseFloat(formTotal.value)*parseFloat(formWeighting.value));
                }

      }
    }
    document.moduleAssess.totalmarks.value=marks/totalweighting*100;
    if (marks>totalweighting) {
         document.moduleAssess.markstotal.value="Error with Marks Total. Total should not be higher than 100";
         return false;
    }
    else document.moduleAssess.markstotal.value="";
    return true;
}

function altervalues() {
    document.moduleAssess.statechange.value="Yes";
    return true;
}

function deleteMarks(form)
{
    if (confirm("Delete Marks: Are you sure?"))
    {
        document.moduleAssess.deletemarksdialog.value="Yes";
        form.submit();
    }
    else
    {
        document.moduleAssess.deletemarksdialog.value="No";
        document.moduleAssess.viewmarksAssStudent.value="Yes";
        form.submit();
    }
    

}

function saveDetails(form)
{
    if (document.moduleAssess.statechange.value=="Yes" && addUpMarkBoxes())
